<div class="col-md-4 col-md-offset-4">
  <h2>Edit discussion board</h2>

  <hr>
  <%= form_for @group do |f| %>

  Title
  <%= f.text_field :title %>
  <br>
  <br>
  Description
  <br>
  <%= f.text_area :description%>
  <br>
  <br>
  <%= f.submit "Submit", :disable_with => 'Submiting...' %>
  <% end %>
</div>
